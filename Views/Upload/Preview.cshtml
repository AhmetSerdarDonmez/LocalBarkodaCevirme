@{
    ViewBag.Title = "Preview PDF";
}

<h2>Preview Your Barcodes</h2>

<!-- The PDF is served by Upload/GetTempPdf -->
<iframe src="@Url.Action("GetTempPdf", "Upload")" style="width:100%; height:600px;" frameborder="0"></iframe>

<div style="margin-top:20px;">
    <a href="@Url.Action("Download", "Upload")" class="btn btn-success">Confirm & Download</a>
    <a href="@Url.Action("Index", "Upload")" class="btn btn-secondary">Cancel</a>
</div>
